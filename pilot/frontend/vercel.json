{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://autoimport-backend.up.railway.app/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://autoimport-backend.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
